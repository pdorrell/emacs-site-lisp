(set-lisp-abbrev "tr" '("(translate " mark ")" goto-mark))

(set-lisp-abbrev "font" '("(setfont (scalefont (findfont 'Helvetica) 10))"))

(set-lisp-abbrev "sl" '("(setlinewidth"))
(set-lisp-abbrev "np" '("(newpath)"))

